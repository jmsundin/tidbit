{
  "name": "tidbit",
  "version": "7.0.0",
  "description": "tidbit shows you content that is relevant to you",
  "main": "src/App.js",
  "scripts": {
    "dev": "parcel ./src/index.html",
    "format": "prettier --write \"./src/**/*.{js,jsx}\"",
    "format:check": "prettier --check \"./src/**/*.{js,jsx}\"",
    "lint": "eslint \"./src/**/*.{js,jsx}\" --quiet"
  },
